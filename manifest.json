{
  "manifest_version": 2,
  "name": "X to FixUp",
  "version": "1.0",
  "description": "Replace X links with Fixup links instead.",
  "permissions": ["clipboardWrite"],
  "content_scripts": [
    {
      "matches": [
        "*://*.x.com/*",
        "*://*.twitter.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}
